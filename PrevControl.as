package  {	import flash.events.MouseEvent;		public class PrevControl extends UnopenableControl {		public function PrevControl(preferredWidth:int, preferredHeight:int, preferredColor:uint, audio:AudioPlayer) {			super(preferredWidth, preferredHeight, preferredColor, audio);			this.addChild(new IMGPrev());			this.addEventListener(MouseEvent.CLICK, this.onClick);		}				public function onClick(e:MouseEvent):void {			this.getAudioPlayer().playPrevious();		}	}	}